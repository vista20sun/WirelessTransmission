<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.yuyang.wirelesstransmission.initActivity">
    <ImageView
        android:layout_width="167dp"
        android:layout_height="164dp"
        app:srcCompat="@drawable/icon_background"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        tools:layout_editor_absoluteX="425dp" />

    <ImageButton
        android:id="@+id/view_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_view"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintRight_toLeftOf="@+id/server_button"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/server_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_button"
        app:layout_constraintLeft_toRightOf="@+id/view_button"
        app:layout_constraintRight_toLeftOf="@+id/set_eButton"
        app:layout_constraintTop_toTopOf="@+id/view_button"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/ic_menu_upload" />

    <ImageButton
        android:id="@+id/his_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_recent_history"
        app:layout_constraintTop_toTopOf="@+id/set_eButton"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/set_eButton"
        android:layout_marginBottom="0dp"
        app:layout_constraintLeft_toRightOf="@+id/set_eButton"
        android:layout_marginLeft="8dp"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageButton
        android:id="@+id/set_eButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_manage"
        app:layout_constraintTop_toTopOf="@+id/server_button"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/server_button"
        android:layout_marginBottom="0dp"
        app:layout_constraintLeft_toRightOf="@+id/server_button"
        app:layout_constraintRight_toLeftOf="@+id/his_Button"
        android:layout_marginLeft="8dp" />

</android.support.constraint.ConstraintLayout>
